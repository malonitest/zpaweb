{
  "name": "zpaweb",
  "version": "1.0.0",
  "description": "Moderní webová stránka pro společnost AutoZástava24, poskytující rychlé půjčky pod zástavu auta.",
  "main": "service-worker.js",
  "scripts": {
    "build": "npm run build:css && npm run inject:tracking && npm run generate:sitemap",
    "build:css": "tailwindcss -i ./src/input.css -o ./styles.css --minify",
    "watch:css": "tailwindcss -i ./src/input.css -o ./styles.css --watch",
    "inject:tracking": "node scripts/inject-tracking.js",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.17",
    "canvas": "^2.11.2",
    "hint": "^7.1.13",
    "tailwindcss": "^4.1.17"
  }
}
